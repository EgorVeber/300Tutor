<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/subjectTestLlContainer"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="@dimen/margin_16"
    android:layout_marginBottom="@dimen/margin_8"
    android:background="@drawable/rectangle_16_content_background"
    android:orientation="horizontal"
    android:paddingVertical="@dimen/margin_4">

    <androidx.cardview.widget.CardView
        android:layout_width="@dimen/size_36"
        android:layout_height="@dimen/size_36"
        android:layout_marginStart="@dimen/margin_12"
        android:backgroundTint="?attr/background"
        app:cardCornerRadius="@dimen/radius_full"
        app:cardElevation="0dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/ic_quality_education"
            app:tint="?attr/primary" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/subjectTestNameTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="@dimen/margin_8"
        android:layout_marginEnd="@dimen/margin_8"
        android:text="@string/logout"
        android:textColor="?attr/textPrimary"
        android:textSize="@dimen/text_16"
        tools:text="asddddddddddddddddddddddddddddddddddddasdasdasdasdas" />
</androidx.appcompat.widget.LinearLayoutCompat>
